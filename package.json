{
  "name": "aigents-uno-web",
  "version": "1.0.0",
  "description": "Vercel + Next.js based setup for the AIGents site",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "command -v husky >/dev/null 2>&1 && husky || echo 'Husky not available in production, skipping git hooks setup'",
    "lint": "cd aigents-web && npm run lint",
    "lint:fix": "cd aigents-web && npm run lint:fix",
    "lint-staged": "cd aigents-web && npx eslint --fix",
    "build": "cd aigents-web && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aigentsmx/aigents-uno-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/aigentsmx/aigents-uno-web/issues"
  },
  "homepage": "https://github.com/aigentsmx/aigents-uno-web#readme",
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11"
  },
  "lint-staged": {
    "aigents-web/**/*.{js,jsx,ts,tsx}": [
      "npm run lint-staged"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "@tailwindcss/postcss": "^4"
  }
}
